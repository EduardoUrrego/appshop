<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:name="com.google.android.gms.maps.SupportMapFragment"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fondo"
    tools:context=".maps">

    <Button
        android:id="@+id/button2"
        android:layout_width="55dp"
        android:layout_height="51dp"
        android:background="@drawable/casa2"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="330dp"
        tools:layout_editor_absoluteY="729dp" />

    <TextView
        android:id="@+id/nombre2"
        android:layout_width="194dp"
        android:layout_height="46dp"
        android:text="Pan tajado blanco familiar x 600 gr"
        android:textColor="@color/black"
        android:textStyle="bold"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="37dp"
        tools:layout_editor_absoluteY="123dp" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="138dp"
        android:layout_height="40dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="247dp"
        tools:layout_editor_absoluteY="129dp"
        tools:srcCompat="@drawable/logo" />


    <com.google.android.gms.maps.MapView
        android:id="@+id/mapView"
        android:layout_width="315dp"
        android:layout_height="316dp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="57dp"
        tools:layout_editor_absoluteY="190dp" />

    <ImageView
        android:id="@+id/fondo"
        android:layout_width="336dp"
        android:layout_height="177dp"
        android:background="@color/teal_700"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="46dp"
        tools:layout_editor_absoluteY="534dp"
        tools:srcCompat="@color/teal_700" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="88dp"
        android:layout_height="73dp"
        app:srcCompat="@drawable/telefono"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="47dp"
        tools:layout_editor_absoluteY="547dp" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="94dp"
        android:layout_height="66dp"
        app:srcCompat="@drawable/ubicacion"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="45dp"
        tools:layout_editor_absoluteY="627dp" />

    <EditText
        android:id="@+id/editTextTextPostalAddress"
        android:layout_width="240dp"
        android:layout_height="49dp"
        android:ems="10"
        android:inputType="textPostalAddress"
        android:text="Calle 44BS #72A, Kennedy, BogotÃ¡"
        android:textColor="@color/black"
        android:textSize="14sp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="135dp"
        tools:layout_editor_absoluteY="634dp" />

    <EditText
        android:id="@+id/telefono"
        android:layout_width="151dp"
        android:layout_height="43dp"
        android:editable="false"
        android:ems="10"
        android:inputType="phone"
        android:text="3134486023"
        android:textColor="@color/black"
        android:textSize="18sp"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="135dp"
        tools:layout_editor_absoluteY="560dp" />


</androidx.constraintlayout.widget.ConstraintLayout>