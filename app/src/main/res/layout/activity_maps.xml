<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".maps">

    <TextView
        android:id="@+id/nombre2"
        android:layout_width="194dp"
        android:layout_height="46dp"
        android:text="Pan tajado blanco familiar x 600 gr"
        android:textColor="@color/black"
        android:textStyle="bold"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="37dp"
        tools:layout_editor_absoluteY="123dp" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="138dp"
        android:layout_height="40dp"
        tools:layout_editor_absoluteX="247dp"
        tools:layout_editor_absoluteY="129dp"
        tools:srcCompat="@drawable/logo" />

    <com.google.android.gms.maps.MapView
        android:id="@+id/mapView"
        android:layout_width="317dp"
        android:layout_height="301dp"
        tools:layout_editor_absoluteX="53dp"
        tools:layout_editor_absoluteY="192dp" />

    <EditText
        android:id="@+id/telefono"
        android:layout_width="151dp"
        android:layout_height="43dp"
        android:editable="false"
        android:ems="10"
        android:inputType="phone"
        android:text="3134486023"
        android:textColor="@color/black"
        android:textSize="18sp"
        tools:layout_editor_absoluteX="127dp"
        tools:layout_editor_absoluteY="525dp" />

    <EditText
        android:id="@+id/editTextTextPostalAddress"
        android:layout_width="252dp"
        android:layout_height="51dp"
        android:ems="10"
        android:inputType="textPostalAddress"
        android:text="Calle 44BS #72A, Kennedy, BogotÃ¡"
        android:textColor="@color/black"
        android:textSize="14sp"
        tools:layout_editor_absoluteX="114dp"
        tools:layout_editor_absoluteY="590dp" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="90dp"
        android:layout_height="61dp"
        app:srcCompat="@drawable/ubicacion"
        tools:layout_editor_absoluteX="37dp"
        tools:layout_editor_absoluteY="584dp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="88dp"
        android:layout_height="73dp"
        app:srcCompat="@drawable/telefono"
        tools:layout_editor_absoluteX="39dp"
        tools:layout_editor_absoluteY="509dp" />
</androidx.constraintlayout.widget.ConstraintLayout>